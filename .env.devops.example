# PromptQL app runtime
PROMPTQL_API_KEY=
PROMPTQL_PLAYGROUND_URL=
PROMPTQL_AUTH_TOKEN=
PROMPTQL_AUTH_MODE=public

# Optional Hasura CE target for query_hasura_ce
PROMPTQL_HASURA_GRAPHQL_ENDPOINT=http://hasura:8080/v1/graphql
PROMPTQL_HASURA_ADMIN_SECRET=testsecret

# Optional sample Hasura profile
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=appdb
POSTGRES_PORT=15432
HASURA_PORT=18080
HASURA_GRAPHQL_ADMIN_SECRET=testsecret
HASURA_GRAPHQL_ENABLE_CONSOLE=false
HASURA_GRAPHQL_DEV_MODE=true
HASURA_GRAPHQL_ENABLED_LOG_TYPES=startup,http-log,webhook-log,websocket-log,query-log
